{
  "extends": "eslint-config-airbnb",
  "plugins": ["jsx-a11y"],
  "env": {
    "browser": true,
    "jest": true
  },
  "scripts": {
    "build": "webpack --mode production"
  },
  "rules": {
    "react/react-in-jsx-scope": "off",
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "react/jsx-no-bind": "off"
  }  
}
